{
    "parser": "babel-eslint",

    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
        "react/prop-types": 0,
        "import/prefer-default-export": "off",
        "react/jsx-one-expression-per-line": "off",
        "linebreak-style": 0,
        "no-use-before-define": "off"
    }
}